{
  "name": "adonis-framework",
  "version": "2.0.9",
  "description": "Adonis framework makes it easy for you to write webapps with less code",
  "main": "index.js",
  "scripts": {
    "test": "npm run standard && istanbul cover _mocha --report lcovonly -- -R spec test/unit test/acceptance && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "npm run standard && istanbul cover _mocha test/unit test/acceptance  --bail",
    "standard": "standard src/**/*.js providers/*.js"
  },
  "keywords": [
    "adonis-framework",
    "mvc",
    "mvc-framework"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "adonis-fold": "^2.0.1",
    "chai": "^3.3.0",
    "cheerio": "^0.20.0",
    "co-mocha": "^1.1.2",
    "co-supertest": "0.0.10",
    "coveralls": "^2.11.4",
    "cz-conventional-changelog": "^1.1.5",
    "formidable": "^1.0.17",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "pem": "^1.8.1",
    "standard": "^6.0.0",
    "supertest": "^1.1.0",
    "zombie": "^4.2.1"
  },
  "peerDependencies": {
    "adonis-fold": "^2.0.1"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "cat-log": "^1.0.0",
    "co": "^4.6.0",
    "dotenv": "^2.0.0",
    "eventemitter2": "^0.4.14",
    "lodash": "^4.0.1",
    "mkdirp": "^0.5.1",
    "node-cookie": "^1.0.0",
    "node-req": "^1.0.3",
    "node-res": "^2.0.2",
    "node-static": "^0.7.7",
    "node-uuid": "^1.4.7",
    "nunjucks": "^2.3.0",
    "path-to-regexp": "^1.2.1",
    "require-all": "^2.0.0",
    "type-of-is": "^3.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme"
}
